<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youtu.dao.UserDao">
    <select id="validation" parameterType="com.youtu.entity.User" resultType="com.youtu.entity.User">
        SELECT *
        FROM user
        WHERE user_uuid = #{user_uuid} AND password = #{password};
    </select>
</mapper>