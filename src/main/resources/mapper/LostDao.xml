<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youtu.dao.LostDao">
    <select id="queryById" resultType="Lost" parameterType="int">
        SELECT id,lost_uuid
        FROM lost
        WHERE id = #{id};
    </select>
    <select id="queryAll" resultType="Lost">
        SELECT id,lost_uuid
        FROM lost;
    </select>
</mapper>